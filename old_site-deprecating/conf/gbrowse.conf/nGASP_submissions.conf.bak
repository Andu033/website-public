[CRAIG]
feature      = candidate_gene:CRAIG
glyph        = full_transcript
category     = Submitted Phase 1 gene predictions
height       = 8
forwardcolor = sub { 
		 my $s = lc shift->source;
		 $s =~ s/_.+//;
		 $s =~ s/[^a-z]+//g;
		 my %color = COLOR;
		 $color{$s};
	       }
reversecolor = sub {
                 my $s = lc shift->source;
		 $s =~ s/_.+//;
                 $s =~ s/[^a-z]+//;
                 my %color = COLOR;
                 $color{$s};
               }
utr_color    = lightgrey
label        = 0
description  = sub {
		my $f = shift;
		my $program = lc $f->source;
	        my ($cat,$ver) = $program =~ /\S+?_cat(\S+?)_ver(\S+)/;
	        $program = $cat ? "category $cat" : '';
		$program .= " version $ver"  if $ver;
		$program;
  }
