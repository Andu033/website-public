#stylesheet

[default]
glyph   = box
bump    = 0
bgcolor = cyan
fgcolor = black
font    = sanserif

[Component]
glyph   = anchored_arrow

[Gene]
glyph   = line
style   = hat

[transcription:high]
bump    = 1

[transcription:low]
bump    = 0

[transcript:high]
bump = 1
connector = hat

[transcript:low]
bump = 0
connector = solid

[alignment:high]
glyph = line
bump = 1
style = solid

[alignment:low]
glyph = line
bump = 0
style = solid

[similarity]
bgcolor = yellow

[experimental]
bgcolor = blue
