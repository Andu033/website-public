
Search [% '<span class="species">' _ c.config.sections.species_list.$species.title _ '</span> ' _ pluralize(class) %]
 <form onsubmit='$jq("#basic-search-results").empty(); ajaxGet($jq("#basic-search-results"), "/search/[% class %]/" + $jq("#basic-search-input").attr("value") + "/?species=[% species %]&inline=1&widget=basic_search");return false;'>

    <input class="text" id="basic-search-input" name="query" type="text" value=""> 
    <input class="ui-button" type="submit" value="search">
 </form>
<br />
<br />
<div id="basic-search-results"></div>