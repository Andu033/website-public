<!-- JQuery tabs -->
<script type="text/javascript">
  $(function() {
  $("#tabs").tabs({
      ajaxOptions: {
                 type: "GET",
                 url : "http://dev.wormbase.org:8080/rest/field/gene/WBGene00006763/common_name",
                 dataType: "html",
                 success: function(html){
                       $("#results").append(html);
                       alert(html);
                 },
                 error: function(request,status,error) {
                      alert(request,status,error);
  }



  }
  });


  </script>

