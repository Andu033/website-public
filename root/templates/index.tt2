   [% WRAPPER home_box title="Discussion" class="home-col" id="posts"-%] 
      <div id="discussion"></div>
      <script language="javascript"
      src = "http://dev.wormbase.org/db/misc/inline_feed_jquery?src=http%3A%2F%2Fforums.wormbase.org%2Findex.php%3Faction%3D.xml&show_board=1;link_board=1;show_poster=1;link_poster=1;load_box=discussion"
      type="text/javascript">
      </script>
      <div class="older-news">Read more on <a href="http://forums.wormbase.org/">The Worm Community Forum</a>&nbsp;&raquo;</div>
   [% END %]

   [% WRAPPER home_box title="Events" class="home-col" id="events"-%] 
      Link this to something.
   [% END %]

   [% WRAPPER home_box title="Issues" id="posts" class="home-col" -%] 
     <div id="rss-box">
      [% USE date %]
      [% FOREACH IS IN issues %]
	<div class="entry"> 
	<h3><a href="[% c.uri_for(c.controller('tool').action_for('issue'),IS.id) %]">&raquo; [% IF IS.re %]Re: [% END %][% IS.title %]</a></h3>
	[% array=IS.location.split('/') %]
	<span class="byline">Reported on: [% text2link(array.3,array.4, array.6 _ ' ' _ array.3 _ ' page ' _ array.5 _ ' section') %] by <a href="">[% username(IS.people) %]</a> [% date.format(IS.time, '%B %d, %Y, %I:%M %P','en_GB') %]</span>
	</div>
      [% END %]
    </div>
     look for this sign<a   tip="report a problem" class="tip-simple feed ui-corner-all"  rel="[% c.uri_for('/rest','feed','issue','main') %]"><span class="ui-icon ui-icon-notice ui-button"></span></a>
     <div class="older-news">Read more on <a href="[% c.uri_for('/tools/issues') %]">The WormBase Issue Archive</a>&nbsp;&raquo;</div>
    [% END %]



</div>