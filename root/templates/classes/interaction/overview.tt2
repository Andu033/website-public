<h2>[% fields.name.data.label %] <span>([% fields.type.data %])</span></h2>

[%

WRAPPER highlight_box;

END;

  '<div class="description">';
   IF fields.remarks.data.0.text.defined;
      FOREACH obj IN fields.remarks.data;
        evidence(obj.evidence, '', obj.text);
      END;
   ELSE;
      markup(fields.remarks.data.join('<br />'),0);
   END;
  '</div>';





