	tags2link(fields.phenotypes.data);
END;



WRAPPER $field_block title="interactor" key="interactor";
	tags2link(fields.interactor.data);
END;

WRAPPER $field_block title="RNAi" key="rnai";
    tags2link(fields.rnai.data);
END;


%]